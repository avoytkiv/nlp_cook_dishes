{	"name": "Python 3",
	"build": {
		"dockerfile": "Dockerfile",
		"context": "..",
		"args": {
			"VARIANT": "3.11"
		}
	},
	"customizations": {
		"vscode": {
			"settings": {
				"python.defaultInterpreterPath": "/home/vscode/venv/bin/python"
			},
			"extensions": [
				"ms-toolsai.jupyter",
				"ms-python.python",
				"GitHub.copilot",
				"ms-vscode-remote.remote-ssh"
			]
		}
	}
}
